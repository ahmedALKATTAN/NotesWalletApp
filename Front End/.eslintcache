}\n      });\n    } else {\n      axios.post(\"http://localhost:4000/app/login\", Info).then((data) => {\n        setIsLoading(false);\n        console.log(data.data.walletdata);\n\n        if (data.data.success) {\n          authCtx.isLoggedIn = true;\n          authCtx.login(data.data.token, data.data.walletdata);\n\n          history.push(\"/profile\");\n        } else {\n          alert(data.data.messeg);\n        }\n      });\n    }\n  };\n\n  return (\n    <section className={classes.auth}>\n      <h1>{isLogin ? \"Login\" : \"Sign Up\"}</h1>\n      <form>\n        <div className={classes.control}>\n          <label htmlFor=\"email\">Your Email</label>\n          <input\n            name=\"email\"\n            type=\"email\"\n            id=\"email\"\n            required\n            onChange={handelChange}\n            value={Info.email}\n          />\n        </div>\n        <div className={classes.control}>\n          <label htmlFor=\"password\">Your Password</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            name=\"password\"\n            required\n            onChange={handelChange}\n            value={Info.password}\n          />\n        </div>\n        <div className={classes.actions}>\n          {!isLoading && (\n            <button onClick={submitTheData}>\n              {isLogin ? \"Login\" : \"Create Account\"}\n            </button>\n          )}\n\n          {isLoading && <p>Sending Request .......</p>}\n\n          <button\n            type=\"button\"\n            className={classes.toggle}\n            onClick={switchAuthModeHandler}\n          >\n            {isLogin ? \"Create new account\" : \"Login with existing account\"}\n          </button>\n        </div>\n      </form>\n    </section>\n  );\n};\n\nexport default AuthForm;\n","D:\\React Final Project 29-11-2021\\Front End File\\src\\components\\Profile\\ProfileForm.js",["244","245"],"import { useContext, useRef, useState } from \"react\";\